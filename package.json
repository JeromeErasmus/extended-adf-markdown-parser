{
  "name": "@extended-adf/parser",
  "version": "0.1.0",
  "description": "Bidirectional parser for ADF and Extended Markdown",
  "packageManager": "yarn@4.7.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:watch": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "yarn test && yarn build"
  },
  "keywords": [
    "adf",
    "atlassian",
    "markdown",
    "parser",
    "confluence",
    "jira"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Jerome Erasmus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jerome/extended-adf-markdown-parser"
  },
  "bugs": {
    "url": "https://github.com/jerome/extended-adf-markdown-parser/issues"
  },
  "homepage": "https://github.com/jerome/extended-adf-markdown-parser#readme",
  "dependencies": {
    "ajv": "^8.17.1",
    "micromark": "^4.0.2",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/jest": "^30.0.0",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.5.0",
    "@types/unist": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.1.3",
    "jest-util": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
